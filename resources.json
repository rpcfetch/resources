{
    "version": "0.1.0",
    "app": [
        {
            "pattern": "discord",
            "regex": false,
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/app/discord.png"
        },
        {
            "pattern": "^Code( - [a-zA-Z]+)?$",
            "regex": true,
            "icase": false,
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/app/vscode.png"
        },
        {
            "pattern": "firefox",
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/app/firefox.png"
        },
        {
            "pattern": "Gimp",
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/app/gimp.png"
        }
    ],
    "os": [
        {
            "pattern": "^Arch( Linux)?$",
            "regex": true,
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/os/archlinux.png"
        },
        {
            "pattern": "NixOS",
            "regex": false,
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/os/nixos.png"
        }
    ],
    "wm": [
        {
            "pattern": "dwm",
            "regex": false,
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/wm/dwm.png"
        },
        {
            "pattern": "Hyprland",
            "regex": true,
            "image": "https://raw.githubusercontent.com/rpcfetch/resources/main/assets/wm/hyprland.png"
        }
    ]
}
